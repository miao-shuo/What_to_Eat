from random import choice

def main():
    meat_dishes = ['椒麻鸡', '酸汤肥牛', '牛尾汤', '回锅肉', '咖喱牛肉', '青豆炒虾仁', '红烧牛腩', '小炒肉', '泡椒牛肉']
    vegetable_dishes = ['蚝油生菜', '蒜炒菠菜', '青椒炒蛋', '粉丝包菜', '清炒苕尖', '番茄炒蛋']

    meat_dishe = choice(meat_dishes)
    vegetable_dishe = choice(vegetable_dishes)

    print('今天吃：', meat_dishe, vegetable_dishe)

if __name__ == '__main__':
    main()
